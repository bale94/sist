<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- vo객체 alias 지정 -->
	<typeAliases>
		<typeAlias alias="memberdb" type="memberDB.vo.MemberDB"/>
		
	</typeAliases>
	<!-- 사용할 sql파일 처리
		업무별 기능 분류로 sql 파일을 분리시켜서 처리
		사원정보의 리스트, 수정, 등록, 삭제, 상세화면조회: /EmpMapper.xml
		게시판의 리스트, 수정, 답글달기, 삭제 등: /BoardMapper.xml
	 -->
	<mappers>
		<mapper resource="resource/mapper/Member/MemberMapper.xml"/>
	</mappers>
	
</configuration>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
   
<mapper namespace="memberDB.dao.MemberDao">
	<insert id="memberJoin">
		insert into snmember values(#{email},#{pass},#{name},#{birth},#{phone},#{address},#{auth})
	</insert>
	<select id="emailCheck" resultType="int">
		select count(*) from snmember where email = #{email}
	</select>

</mapper>
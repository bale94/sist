<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="springweb.a02_mvc.a03_dao.EmpDao">
	<select id="getEmpList" resultType="emp" 
							parameterType="emp">
		select * from emp
	</select>
</mapper>
